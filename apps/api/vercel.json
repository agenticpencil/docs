{
  "version": 2,
  "rewrites": [
    { "source": "/v1/(.*)", "destination": "/api/index" },
    { "source": "/health", "destination": "/api/index" },
    { "source": "/", "destination": "/api/index" }
  ],
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.2.0",
      "maxDuration": 60
    },
    "api/*.js": {
      "runtime": "@vercel/node@3.2.0"
    }
  }
}
